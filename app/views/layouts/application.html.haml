!!!
%html
  %head
    = analytics_init if Rails.env.production?
    %meta{:content => "width=device-width, initial-scale=1", :name => "viewport"}
    %link{:href => image_path('moonlight-logo.png'), :rel => "shortcut icon", :type => "image/x-icon"}
    %link{:href => image_path('moonlight-logo.png'), :rel => "icon", :type => "image/x-icon"}
    %title Moonlight Garden Supply
    = stylesheet_link_tag "application", media: "all"
    = javascript_include_tag "application"
    %script{:src => "//maps.google.com/maps/api/js?v=3.13&amp;sensor=false&amp;libraries=geometry", :type => "text/javascript"}
    %script{:src => "//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js", :type => "text/javascript"}
    = csrf_meta_tags
  %body
    %div#header
      = render :partial => "shared/bflash"
      %header#main_header
        = render :partial => "layouts/header"
    #main-content.container-fluid
      = yield
    %footer#footer.text-center.font-m
      Built With
      = fa_icon "heart"
      By:
      %a{href: 'http://www.cottonwoodcoding.com', target: '_blank'} Cottonwood Coding
